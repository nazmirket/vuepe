{
   "name": "vue-page-editor",
   "author": "nazmiyilmaz",
   "version": "1.2.8",
   "description": "A multi purpose page editor plugin for Vue2",
   "main": "dist/vue-page-editor.ssr.js",
   "browser": "dist/vue-page-editor.esm.js",
   "module": "dist/vue-page-editor.esm.js",
   "unpkg": "dist/vue-page-editor.min.js",
   "files": [
      "dist/*",
      "src/**/*.vue",
      "public/*"
   ],
   "sideEffects": false,
   "scripts": {
      "serve": "vue-cli-service serve dev/serve.js",
      "prebuild": "rimraf ./dist",
      "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
      "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
      "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
      "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
   },
   "dependencies": {
      "@interactjs/actions": "^1.10.11",
      "@interactjs/auto-start": "^1.10.11",
      "@interactjs/dev-tools": "^1.10.11",
      "@interactjs/inertia": "^1.10.11",
      "@interactjs/interact": "^1.10.11",
      "@interactjs/modifiers": "^1.10.11"
   },
   "devDependencies": {
      "@babel/core": "^7.14.6",
      "@babel/preset-env": "^7.14.7",
      "@rollup/plugin-alias": "^3.1.2",
      "@rollup/plugin-babel": "^5.3.0",
      "@rollup/plugin-commonjs": "^14.0.0",
      "@rollup/plugin-node-resolve": "^9.0.0",
      "@rollup/plugin-replace": "^2.4.2",
      "@rollup/plugin-url": "^6.1.0",
      "@vue/cli-plugin-babel": "^4.5.13",
      "@vue/cli-service": "^4.5.13",
      "cross-env": "^7.0.3",
      "minimist": "^1.2.5",
      "node-sass": "^6.0.1",
      "rimraf": "^3.0.2",
      "rollup": "^2.52.8",
      "rollup-plugin-copy": "^3.4.0",
      "rollup-plugin-scss": "^2.6.1",
      "rollup-plugin-terser": "^7.0.2",
      "rollup-plugin-vue": "^5.1.9",
      "sass-loader": "^12.1.0",
      "vue": "^2.6.14",
      "vue-template-compiler": "^2.6.14"
   },
   "peerDependencies": {
      "vue": "^2.6.14"
   },
   "engines": {
      "node": ">=14"
   }
}
